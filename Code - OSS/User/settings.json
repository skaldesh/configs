{
    "[yaml]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 4,
        "editor.autoIndent": "advanced"
    },
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "window.menuBarVisibility": "compact",
    "window.zoomLevel": 2,
    "workbench.iconTheme": "simple-icons",
    //"go.formatTool": "goimports",
    //"go.autocompleteUnimportedPackages": true,
    "go.toolsGopath": "/home/skaldesh/go",
    "go.useLanguageServer": true,
    "go.addTags": {
        "tags": "json,yaml",
        "options": "json=omitempty,yaml=inline",
        "promptForTags": true,
        "transform": "snakecase"
    },
    "[go]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true,
            "source.fixAll": true,
        },
        // Optional: Disable snippets, as they conflict with completion ranking.
        "editor.snippetSuggestions": "none",
    },
    "[go.mod]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true,
        },
    },
    "gopls": {
         // Add parameter placeholders when completing a function.
        "usePlaceholders": true,
    
        // If true, enable additional analyses with staticcheck.
        // Warning: This will significantly increase memory usage.
        "staticcheck": false,

        "analyses": {
            "fillreturns": true,        
            "nonewvars": true,
        }
    },
    "editor.fontSize": 13,
    "search.useIgnoreFiles": false,
    "explorer.confirmDragAndDrop": false,
    "keyboard.dispatch": "keyCode",
    "licenser.license": "MIT",
    "licenser.projectName": "GML",
    "window.openFoldersInNewWindow": "off",
    "npm.fetchOnlinePackageInfo": false,
    "workbench.enableExperiments": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "update.mode": "start",
    "terminal.integrated.tabs.enabled": true,
    "workbench.editorAssociations": {
        "*.html": "default",
        "*.onnx": "hexEditor.hexedit"
    },
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": false
    },
    "editor.detectIndentation": false,
    "dart.lineLength": 120,
    "editor.wordWrapColumn": 130,
    "editor.wrappingIndent": "deepIndent",
    "telemetry.telemetryLevel": "off",
    "zls.warn_style": true,
    "zls.enable_unused_variable_warnings": true,
    "zls.enable_snippets": true,
    "zls.path": "/usr/local/bin/zls",
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "redhat.telemetry.enabled": false,
    "go.diagnostic.vulncheck": "Imports",
    "git.openRepositoryInParentFolders": "never",
}